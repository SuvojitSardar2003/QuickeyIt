{
    "version" : 2,
    "name" : "QuickeyIt",
    "builds" : [
        {
            "src" : "./index.js",
            "use" : "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes" : [
        { 
            "src": "/api/(.*)", 
            "dest": "/index.js" 
        },
        {
            "src" : "/(.*)",
            "dest" : "/"
        }
    ]
}